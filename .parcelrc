{
  "extends": "@parcel/config-default",
  "resolvers": ["@parcel/resolver-glob", "..."],
  "transformers": {
    "*.ejs": ["parcel-transformer-ejs"],
    "*.hbs": ["parcel-transformer-handlebars"],
    "*.glsl *.frag .vert": ["@parcel/transformer-glsl"],
    "files-to-bypass/*.*": ["parcel-transformer-bypass"]
  }
}